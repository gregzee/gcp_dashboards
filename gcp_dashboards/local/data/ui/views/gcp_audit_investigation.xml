<form>
  <label>GCP Audit Investigation</label>
  <description>v1.2</description>
  <init>
    <set token="minspan">1</set>
    <set token="show_annotations">*</set>
  </init>
  <search id="audit_selection_base">
    <query>
       |tstats fillnull_value="null" count where `gcp_index` sourcetype=google:gcp:pubsub:* $search_text_tok$ "data.logName"="*activity" OR "data.logName"="*data_access" OR "data.logName"="*system_event" 
       by _time, sourcetype, data.resource.labels.project_id,data.logName,data.protoPayload.resourceName, data.resource.type, "data.protoPayload.authenticationInfo.principalEmail",data.protoPayload.serviceName, data.protoPayload.methodName, data.protoPayload.requestMetadata.callerIp, data.protoPayload.requestMetadata.callerSuppliedUserAgent span=$secs_for_bin$s
       | rename data.resource.labels.project_id as ProjectID, data.logName as LogName, data.protoPayload.authenticationInfo.principalEmail as Email,data.protoPayload.serviceName as ServiceName, data.protoPayload.resourceName as ResourceName, data.resource.type as ResourceType, data.protoPayload.methodName as MethodName, data.protoPayload.requestMetadata.callerIp as CallerIP, data.protoPayload.requestMetadata.callerSuppliedUserAgent as UserAgent
    </query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  <search id="annotation_count" base="annotations">
    <query>
      stats count
    </query>
    <done>
      <eval token="annotateMarker">if($result.count$&gt;1000,"N","Y")</eval>
    </done>
  </search>
  <fieldset submitButton="false" autoRun="false">
    <input type="time" token="time" searchWhenChanged="true">
      <label>Time</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
      <change>
        <eval token="form.secs_for_bin">if(isnull($time.latest$),$minspan$, if((round(relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)&lt;$minspan$,$minspan$,round(((relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)/$minspan$)*$minspan$))</eval>
        <eval token="time_str">strftime($selection.earliest$,"%m/%d/%y %H:%M:%S")</eval>
        <eval token="time_end">strftime($selection.latest$,"%m/%d/%y %H:%M:%S")</eval>
      </change>
    </input>
    <input type="multiselect" token="project_tok" searchWhenChanged="true">
      <label>Project</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <delimiter> </delimiter>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>ProjectID</fieldForValue>
      <search base="audit_selection_base">
        <query>stats count by ProjectID |eval Label=ProjectID+"("+tostring(count)+")"</query>
        <progress>
          <eval token="form.secs_for_bin">if(isnull($time.latest$),$minspan$, if((round(relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)&lt;$minspan$,$minspan$,round(((relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)/$minspan$)*$minspan$))</eval>
        </progress>
      </search>
    </input>
    <input type="dropdown" token="sourcetype_tok" searchWhenChanged="true">
      <label>Source Type</label>
      <choice value="google:gcp:pubsub:*">ALL</choice>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>sourcetype</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$)|stats count by sourcetype |eval Label=sourcetype+"("+tostring(count)+")"</query>
      </search>
    </input>
    <input type="dropdown" token="logname_tok" searchWhenChanged="true">
      <label>Audit Log</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>LogName</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$)|stats count by LogName|eval Label=LogName+"("+tostring(count)+")"</query>
      </search>
    </input>
    <input type="dropdown" token="service_tok" searchWhenChanged="true">
      <label>Service Name</label>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>ServiceName</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$)|stats count by ServiceName|eval Label=ServiceName+"("+tostring(count)+")"</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="type_tok" searchWhenChanged="true">
      <label>Resource Type</label>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>ResourceType</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$)|stats count by ResourceType|eval Label=ResourceType+"("+tostring(count)+")"</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
    </input>
    <input type="dropdown" token="name_tok" searchWhenChanged="true">
      <label>Resource Name</label>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>ResourceName</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND ResourceType IN ($type_tok$)| stats count by ResourceName|eval Label=ResourceName+"("+tostring(count)+")"</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="method_tok" searchWhenChanged="true">
      <label>Method Name</label>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>MethodName</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$)|stats count by MethodName|eval Label=MethodName+"("+tostring(count)+")"</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="email_tok" searchWhenChanged="true">
      <label>Principal Email</label>
      <fieldForLabel>Label</fieldForLabel>
      <fieldForValue>Email</fieldForValue>
      <search base="audit_selection_base">
        <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$)|stats count by Email|eval Label=Email+"("+tostring(count)+")"</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="search_text_tok" searchWhenChanged="true">
      <label>Search For Text</label>
      <default>*</default>
    </input>
    <input type="dropdown" token="time_resolution" searchWhenChanged="true">
      <label>Chart resolution</label>
      <choice value="10">Crude</choice>
      <choice value="50">Low</choice>
      <choice value="70">Medium</choice>
      <choice value="150">High</choice>
      <choice value="600">Ultra</choice>
      <change>
        <eval token="form.secs_for_bin">if(isnull($time.latest$),$minspan$, if((round(relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)&lt;$minspan$,$minspan$,round(((relative_time(now(), $time.latest$)-relative_time(now(), $time.earliest$))/$time_resolution$)/$minspan$)*$minspan$))</eval>
      </change>
      <default>70</default>
    </input>
    <input type="text" token="secs_for_bin" depends="$never$">
      <label>secs_for_bin</label>
      <default>1200</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Audit Events : $time_str$ - $time_end$</title>
      <input type="dropdown" token="by_token">
        <label>Arrange Chart By:</label>
        <choice value="ServiceName">Service Name</choice>
        <choice value="ResourceType">Resource Type</choice>
        <choice value="ResourceName">Resource Name</choice>
        <choice value="MethodName">Method Name</choice>
        <default>ResourceType</default>
        <initialValue>ResourceType</initialValue>
      </input>
      <input type="dropdown" token="annotate_category_by_token" searchWhenChanged="true">
        <label>Annotation Label:</label>
        <choice value="ServiceName">Service Name</choice>
        <choice value="ResourceType">Resource Type</choice>
        <choice value="ResourceName">Resource Name</choice>
        <choice value="MethodName">Method Name</choice>
        <default>ResourceName</default>
        <initialValue>ResourceName</initialValue>
      </input>
      <input type="dropdown" token="annotate_detail_by_token" searchWhenChanged="true">
        <label>Annotation Detail:</label>
        <choice value="ServiceName">Service Name</choice>
        <choice value="ResourceType">Resource Type</choice>
        <choice value="ResourceName">Resource Name</choice>
        <choice value="MethodName">Method Name</choice>
        <choice value="Email">Email</choice>
        <choice value="CallerIp">Caller IP</choice>
        <default>MethodName</default>
        <initialValue>MethodName</initialValue>
      </input>
      <input type="checkbox" token="show_annotations_cb" searchWhenChanged="true">
        <label>Annotations</label>
        <choice value="On">On</choice>
        <initialValue>On</initialValue>
        <change>
          <eval token="show_annotations">if(isnull($show_annotations_cb$),"somethingthatwouldnotreallybeinalogfileyoushouldthink","*")</eval>
        </change>
        <delimiter> </delimiter>
      </input>
      <chart>
        <search base="audit_selection_base">
          <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND Email IN ($email_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$) | timechart span=$secs_for_bin$s sum(count) by $by_token$ limit=100</query>
        </search>
        <search base="annotations" type="annotation"></search>
        <selection>
          <set token="selection.earliest">$start$</set>
          <set token="selection.latest">$end$</set>
          <eval token="time_str">strftime($selection.earliest$,"%m/%d/%y %H:%M:%S")</eval>
          <eval token="time_end">strftime($selection.latest$,"%m/%d/%y %H:%M:%S")</eval>
        </selection>
        <option name="charting.chart">column</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.legend.placement">top</option>
        <option name="refresh.display">progressbar</option>
        <drilldown>
          <set token="clicktime">$click.name$</set>
          <set token="clicktimevalue">$click.value$</set>
          <set token="click1drillvalue">$click.name2$</set>
          <eval token="secs_for_bin_rtr">rtrim($secs_for_bin$,"s")</eval>
          <eval token="parm_secs">trim($secs_for_bin_rtr$,"span=")</eval>
          <eval token="url_earliest">if($clicktime$=="_time", $click.value$,$time.earliest$)</eval>
          <eval token="url_latest">if($clicktime$=="_time", $click.value$+$parm_secs$,$time.latest$)</eval>
          <eval token="service_parm">if($by_token$=="ServiceName",$click1drillvalue$,$service_tok$)</eval>
          <eval token="type_parm">if($by_token$=="ResourceType",$click1drillvalue$,$type_tok$)</eval>
          <eval token="name_parm">if($by_token$=="ResourceName",$click1drillvalue$,$name_tok$)</eval>
          <eval token="method_parm">if($by_token$=="MethodName",$click1drillvalue$,$method_tok$)</eval>
          <link target="_blank">gcp_audit_investigation?form.secs_for_bin=$secs_for_bin_rtr$&amp;form.project_tok=$project_tok$&amp;form.sourcetype_tok=$sourcetype_tok$&amp;form.logname_tok=$logname_tok$&amp;form.service_tok=$service_parm$&amp;form.type_tok=$type_parm$&amp;form.name_tok=$name_parm$&amp;form.method_tok=$method_parm$&amp;form.email_tok=$email_tok$&amp;form.by_token=$by_token$&amp;form.annotate_category_by_token=$annotate_category_by_token$&amp;form.annotate_detail_by_token=$annotate_detail_by_token$&amp;form.search_text_tok=$search_text_tok$&amp;form.time.earliest=$url_earliest$&amp;form.time.latest=$url_latest$&amp;form.time_resolution=$time_resolution$</link>
        </drilldown>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <center>
          Click on chart to drill into detailed selection or legend to select category only <br/>
          <a href="gcp_audit_investigation?form.secs_for_bin=$secs_for_bin$&amp;form.project_tok=$project_tok$&amp;form.sourcetype_tok=$sourcetype_tok$&amp;form.logname_tok=$logname_tok$&amp;form.service_tok=$service_tok$&amp;form.type_tok=$type_tok$&amp;form.name_tok=$name_tok$&amp;form.method_tok=$method_tok$&amp;form.email_tok=$email_tok$&amp;form.by_token=$by_token$&amp;form.annotate_category_by_token=$annotate_category_by_token$&amp;form.annotate_detail_by_token=$annotate_detail_by_token$&amp;form.search_text_tok=$search_text_tok$&amp;form.time.earliest=$selection.earliest$&amp;form.time.latest=$selection.latest$&amp;form.time_resolution=$time_resolution$" target="_blank">Click here to open selection in above area in new window</a> <br/>
          
          <a href="search?q=%60gcp_index%60%20sourcetype%3D$sourcetype_tok$%20%22data.logName%22%3D%22%2Aactivity%22%20OR%20%22data.logName%22%3D%22%2Aactivity%22%20OR%20%22data.logName%22%3D%22%2Adata_access%22%20OR%20%22data.logName%22%3D%22%2Asystem_event%22%20data.resource.labels.project_id%20IN%20%28$project_tok$%29%20AND%20data.logName%20IN%20%28$logname_tok$%29%20AND%20data.protoPayload.authenticationInfo.principalEmail%20IN%20%28$email_tok$%29%20AND%20data.protoPayload.serviceName%20IN%20%28$service_tok$%29%20AND%20data.protoPayload.resourceName%20IN%20%28$name_tok$%29%20AND%20data.resource.type%20IN%20%28$type_tok$%29%20%20$search_text_tok$%0A&amp;earliest=$selection.earliest$&amp;latest=$selection.latest$" target="_blank">Click here to open as a Search</a>
          
        </center>
        
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>Email Principal</title>
      <chart>
        <search base="audit_selection_base">
          <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND Email IN ($email_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$) | timechart span=$secs_for_bin$s sum(count) by Email limit=50</query>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">top</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>User Agent</title>
      <chart>
        <search base="audit_selection_base">
          <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND Email IN ($email_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$) | timechart span=$secs_for_bin$s sum(count) by UserAgent limit=100</query>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">top</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>Caller IP</title>
      <chart>
        <search base="audit_selection_base">
          <query>search ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND Email IN ($email_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$) | timechart span=$secs_for_bin$s sum(count) by CallerIP limit=100</query>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">top</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row depends="$noshow$">
    <panel>
      <table>
        <search base="audit_selection_base" id="annotations">
          <query>|search $show_annotations$ ProjectID IN ($project_tok$) AND sourcetype IN ($sourcetype_tok$) AND LogName IN ($logname_tok$) AND Email IN ($email_tok$) AND ServiceName IN ($service_tok$) AND ResourceName IN ($name_tok$) AND ResourceType IN ($type_tok$) 
|eval annotation_label='$annotate_category_by_token$'
|eval annotation_category='$annotate_detail_by_token$'
|top limit=1000 _time, annotation_label,annotation_category 
|table _time, annotation*</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
</form>