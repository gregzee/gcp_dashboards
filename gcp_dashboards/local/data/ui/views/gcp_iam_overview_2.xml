<dashboard>
  <label>IAM Overview</label>
  <search id="tstats_query">
    <query>
      |makeresults 
      |eval usetstats="|tstats fillnull_value=\"null\" count where" |eval notstats=" "
      |eval tstatstag=`tstatstag`
      |eval tstats_post_midtag=if(tstatstag!=" "," ","| fillnull value=\"null\" _time, $datatag$resource.labels.project_id, $datatag$logName, $datatag$$datatag$protoPayload.resourceName, $datatag$resource.type, $datatag$$datatag$protoPayload.authenticationInfo.principalEmail, $datatag$$datatag$protoPayload.serviceName, $datatag$$datatag$protoPayload.methodName, $datatag$$datatag$protoPayload.requestMetadata.callerIp, $datatag$$datatag$protoPayload.requestMetadata.callerSuppliedUserAgent | stats count")
      |eval tstats_post_endtag=if(tstatstag!=" ", "span=$secs_for_bin$s", "")
    </query>
    <done>
      <set token="tstatstag">$result.tstatstag$</set>
      <set token="tstats_post_midtag">$result.tstats_post_midtag$</set>
      <set token="tstats_post_endtag">$result.tstats_post_endtag$</set>
    </done>

  <row>
